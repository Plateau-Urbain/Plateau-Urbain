{
    "name": "Geoffrey/www",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/", "SymfonyStandard\\": "app/SymfonyStandard/"
        }
    },
    "require-dev":{
        "phpunit/phpunit":"*",
        "composer/composer":"*",
        "composer-plugin-api":"*"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/symfony": "2.8.44",
        "doctrine/orm": "^2.4.8",
        "doctrine/dbal": "~2.4",
        "doctrine/doctrine-bundle": "~1.4",
	"doctrine/migrations": "^1.1",
        "doctrine/doctrine-migrations-bundle": "~1.2.1",
        "doctrine/doctrine-fixtures-bundle": "~2.2.0",
        "twig/extensions": "~1.2",
        "symfony/assetic-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "2.5.5",
        "symfony/monolog-bundle": "~2.8",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "1.3.*",
        "sonata-project/admin-bundle": "3.38.3",
        "sonata-project/doctrine-orm-admin-bundle": "~3.0",
        "sonata-project/easy-extends-bundle": "~2.2",
        "sonata-project/user-bundle": "3.6.0",
        "hautelook/alice-bundle": "0.2",
        "ornicar/apc-bundle": "1.0.4",
        "liip/imagine-bundle": "1.9.1",
        "vich/uploader-bundle": "1.7.1",
        "knplabs/knp-gaufrette-bundle": "0.5.3",
        "cspoo/swiftmailer-mailgun-bundle": "~0.3.1",
        "knplabs/knp-paginator-bundle": "v2.8.0",
        "knplabs/knp-menu-bundle": "v2.2.1",
        "stof/doctrine-extensions-bundle": "^1.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
