            <div class="breadcrumb-header">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            <ul class="breadcrumb">
                                <li><a href="{{ path('homepage') }}">Accueil</a></li>
                                {% block breadcrumb_items %}{% endblock %}
