<head>
	<meta charset="utf-8" />

	<!-- Page Title -->
	<title>{% block title %}Plateau Urbain{% endblock %}</title>

	<meta name="keywords" content="plateau, urbain" />
	<meta name="description" content="Créée en juillet 2013, Plateau Urbain est une association proposant de jouer le rôle d’interface entre propriétaires et porteurs de projets, pour redonner vie à des immeubles vacants et soutenir des projets associatifs, entreprenariaux et culturels" />

	<!-- Mobile Meta Tag -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />

	<!-- Fav and touch icons -->
	<link rel="shortcut icon" type="image/x-icon" href="{{ asset('/bundles/app/images/fav_touch_icons/favicon.ico') }}" />

	<!-- IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->

	<!-- Google-Fonts Embeded -->
	 <!-- Raleway -->
	 {% stylesheets  'bundles/app/css/Raleway.css' %}
	  <link rel="stylesheet" href="{{ asset_url }}?family=Raleway:300,500,900%7COpen+Sans:400,700,400italic" media="all" />
	 {% endstylesheets  %}
	 <!-- Roboto -->
	 {% stylesheets  'bundles/app/css/Roboto.css' %}
	  <link rel="stylesheet" href="{{ asset_url }}?family=Roboto:300,400,500,700" media="all" />
	 {% endstylesheets  %}
    {% stylesheets  'bundles/app/css/Ginka.css' %}
        <link rel="stylesheet" href="{{ asset_url }}?family=ginka:300,400,500,600,700" media="all" />
    {% endstylesheets  %}


	<!-- Template CSS -->
        {% stylesheets  'bundles/app/css/bootstrap.min.css'
                        'bundles/app/css/icons.css'
                        'bundles/app/css/font-awesome.min.css'
                        'bundles/app/css/cozy-real-estate-font.css'
                        'bundles/app/css/owl.transitions.css'
                        'bundles/app/css/colorbox.css'
                        'bundles/app/rs-plugin/css/settings.css'
                        'bundles/app/css/style.css'
                        'bundles/app/css/cycle.css'
                        'bundles/app/css/main.css'

        filter='cssrewrite' %}
           <link rel="stylesheet" href="{{ asset_url }}" media="all" />
        {% endstylesheets  %}


        {% block stylesheets %}{% endblock %}

		<!-- Cookie-Consent -->
		{% stylesheets  'bundles/app/css/cookieconsent.min.css' %}
		<link rel="stylesheet" href="{{ asset_url }}" medial="all" />
		{% endstylesheets %}
        {% javascripts  'bundles/app/js/cookieconsent.min.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts  %}
	<script>
		window.addEventListener("load", function(){
		window.cookieconsent.initialise({
		  "palette": {
			"popup": {
			  "background": "#edeff5",
			  "text": "#838391"
			},
			"button": {
			  "background": "#bc1b38"
			}
		  },
		  "content": {
			"message": "Pour améliorer votre navigation, ce site utilise des cookies",
			"dismiss": "OK!",
			"link": "En savoir plus",
			"href": "/cgu"
		  }
		})});
	</script>


        <!-- Modernizr -->
        {% javascripts  'bundles/app/js/modernizr-2.8.1.min.js' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts  %}

