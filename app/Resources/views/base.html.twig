<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
<head>
	<meta charset="utf-8" />

	<!-- Page Title -->
	<title>{% block title %}Plateau Urbain{% endblock %}</title>

	<meta name="keywords" content="real estate, responsive, retina ready, modern html5 template, bootstrap, css3" />
	<meta name="description" content="Cozy - Responsive Real Estate HTML5 Template" />
	<meta name="author" content="Wisely Themes - www.wiselythemes.com" />

	<!-- Mobile Meta Tag -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />

	<!-- Fav and touch icons -->
	<link rel="shortcut icon" type="image/x-icon" href="{{ asset('/bundles/app/images/fav_touch_icons/favicon.ico') }}" />

	<!-- IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->

	<!-- Google Web Font -->
	<link href="http://fonts.googleapis.com/css?family=Raleway:300,500,900%7COpen+Sans:400,700,400italic" rel="stylesheet" type="text/css" />
	<link href='https://fonts.googleapis.com/css?family=Roboto:300,400,500,700' rel='stylesheet' type='text/css' />

	<link href="https://file.myfontastic.com/n6vo44Re5QaWo8oCKShBs7/icons.css" rel="stylesheet">

	<!-- Template CSS -->
        {% stylesheets  'bundles/app/css/bootstrap.min.css'
                        'bundles/app/css/font-awesome.min.css'
                        'bundles/app/css/cozy-real-estate-font.css'
                        'bundles/app/css/owl.transitions.css'
                        'bundles/app/css/colorbox.css'
                        'bundles/app/rs-plugin/css/settings.css'
                        'bundles/app/css/style.css'
												'bundles/app/css/cycle.css'
        filter='cssrewrite' %}
           <link rel="stylesheet" href="{{ asset_url }}" medial="all" />
        {% endstylesheets  %}

				{% stylesheets '@AppBundle/Resources/public/less/main.less'
		    filter='less' %}
					<link rel="stylesheet" href="{{ asset_url }}" medial="all" />
				{% endstylesheets %}

        {% block stylesheets %}{% endblock %}

        <!-- Modernizr -->
        {% javascripts  'bundles/app/js/modernizr-2.8.1.min.js' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts  %}

	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
					(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
				m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

		ga('create', 'UA-74052320-1', 'auto');
		ga('send', 'pageview');

	</script>

	</head>
    <body>
	<!-- BEGIN WRAPPER -->
	<div id="wrapper">

		<!-- BEGIN HEADER -->
		<header id="header">
			<div id="nav-section">
				<div class="container">
					<div class="row">
						<div class="col-sm-12">
							<a href="{{ path('homepage')}}" class="nav-logo"><img src="{{ asset('/public/images/logo.png') }}" alt="Plateau Urbain" /></a>

							<!-- BEGIN MAIN MENU -->
							<nav class="navbar">
								<button id="nav-mobile-btn"><i class="fa fa-bars"></i></button>
								{{ knp_menu_render('AppBundle:Builder:mainMenu') }}
							</nav>
							<!-- END MAIN MENU -->

						</div>
					</div>
				</div>
				<div class="bottom"></div>
			</div>
		</header>
		<!-- END HEADER -->

		<!-- BEGIN PAGE TITLE/BREADCRUMB -->
        {% block breadcrumb %}
            <div class="breadcrumb-header">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            <ul class="breadcrumb">
                                <li><a href="{{ path('homepage') }}">Accueil</a></li>
                                {% block breadcrumb_items %}{% endblock %}
                            </ul>
                            <p class="clearfix"></p>
                        </div>
                    </div>
                </div>
            </div>
        {% endblock %}
        <!-- END PAGE TITLE/BREADCRUMB -->

        {% block prebody %} {% endblock %}

		<div class="content">
			<div class="container pb100">
						{% block containerbkg %} {% endblock %}
        		{% block body %}{% endblock %}
			</div>
		</div>

		{% block actualities %}{% endblock %}

		{% block partners %}{% endblock %}

		{% block footer %}
			{% include '/Partials/footer.html.twig' %}
		{% endblock %}

	</div>
	<!-- END WRAPPER -->

		<!-- Libs -->
        {% javascripts  'bundles/app/js/common.js'
        'bundles/app/js/jquery.prettyPhoto.js'
        'bundles/app/js/owl.carousel.min.js'
        'bundles/app/js/chosen.jquery.min.js'
        'bundles/app/js/infobox.min.js'
        'bundles/app/rs-plugin/js/jquery.themepunch.tools.min.js'
        'bundles/app/rs-plugin/js/jquery.themepunch.revolution.min.js'
        'bundles/app/js/jquery.colorbox.js'
        'bundles/app/js/bootstrap-checkbox.js'
        'bundles/app/js/variables.js'
        'bundles/app/js/scripts.js'
        'bundles/app/js/agencies.js'
        'bundles/app/js/pu.js'
        %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts  %}


		<!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.cycle2/2.1.6/jquery.cycle2.core.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.cycle2/2.1.6/jquery.cycle2.carousel.min.js"></script> -->

		<script src="https://cdnjs.cloudflare.com/ajax/libs/freewall/1.0.5/freewall.min.js"></script>


		{% if app.session.flashbag.has('error_sign') %}
			<script type="text/javascript">
				$(document).ready(function() {
					$.colorbox({html:$('#inline_register_content').html(), width:"400px"});
				});
			</script>
		{% endif %}

		{% block javascript %}
        {% endblock %}

		<div style='display:none'>
			<div id="inline_content">
				{% render(controller("AppBundle:Security:loginForm")) %}
			</div>
			<div id="inline_register_content">
				{% render(controller("FOSUserBundle:Registration:register")) %}
			</div>
		</div>

	</body>
</html>
