

{% block content %}
{{ form_errors(form) }}
<div class="panel panel-success">
    <div class="panel-heading">
        <h2 class="panel-title">INSCRIVEZ VOUS</h2>
    </div>
    <div class="panel-body">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form-horizontal">
            <div class="form-group">
                {{ form_label(form.lastname, null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.lastname) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.firstName , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.firstName) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.birthday , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.birthday) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.email , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.email) }}
                </div>
            </div>
            {% for passwordField in form.plainPassword %}
                <div class="form-group">
                    {{ form_label(passwordField , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                    <div class="col-lg-9">
                        {{ form_widget(passwordField) }}
                    </div>
                </div>

            {% endfor %}


            <div class="form-group">
                {{ form_label(form.company , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.company) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.siret , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.siret) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.address , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.address) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.zipcode , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.zipcode) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.city , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.city) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.phone , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.phone) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.facebookUrl , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.facebookUrl) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.instagramUrl , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.instagramUrl) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.twitterUrl , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.twitterUrl) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.description , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.description) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.wishedSize , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.wishedSize) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.usageType , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.usageType) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.usageDate , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.usageDate) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.usageDuration , null, { 'label_attr': {'class': 'col-lg-3 control-label'} })}}
                <div class="col-lg-9">
                    {{ form_widget(form.usageDuration) }}
                </div>
            </div>
{{ form_rest(form) }}


            <div class="form-actions">
                <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="btn btn-success pull-right" />
            </div>
        </form>
    </div>
</div>
{% endblock content %}
